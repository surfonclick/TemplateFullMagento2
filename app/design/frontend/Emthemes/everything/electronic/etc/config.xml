			</button6_group>-->
		</buttons>
		<product_grid>
		      <grid_desktop>
		            <grid_desktop_onecolumn>5</grid_desktop_onecolumn>
		            <grid_desktop_twocolumns>4</grid_desktop_twocolumns>
		            <grid_desktop_threecolumns>3</grid_desktop_threecolumns>
		      </grid_desktop>
	            <grid_desktop_small>
	                  <grid_small_desktop_onecolumn>4</grid_small_desktop_onecolumn>
	                  <grid_small_desktop_twocolumns>3</grid_small_desktop_twocolumns>
	                  <grid_small_desktop_threecolumns>2</grid_small_desktop_threecolumns>	                  
	            </grid_desktop_small>
	            <grid_tablet>
	                  <grid_tablet_onecolumn>3</grid_tablet_onecolumn>
	                  <grid_tablet_twocolumns>3</grid_tablet_twocolumns>
	                  <grid_tablet_threecolumns>2</grid_tablet_threecolumns>	                  
	            </grid_tablet>
	            <grid_mobile>
	                  <grid_items_mobile>2</grid_items_mobile>	                  
	            </grid_mobile>
	            <grid_element>
	                  <grid_alt_img>thumbnail</grid_alt_img>
                        <grid_image_ratio>1</grid_image_ratio>
	                  <grid_custom_ratio_width>1</grid_custom_ratio_width>
	                  <grid_custom_ratio_height>1</grid_custom_ratio_height>
	                  <grid_alt_img>thumbnail</grid_alt_img>
	                  <grid_show_thumbnail>1</grid_show_thumbnail>
	                  <grid_show_product_name>1</grid_show_product_name>
	                  <grid_auto_product_name>1</grid_auto_product_name>
	                  <grid_show_desc>1</grid_show_desc>
	                  <grid_show_price>1</grid_show_price>
	                  <grid_show_review>1</grid_show_review>
	                  <grid_show_addto>1</grid_show_addto>
	                  <grid_config_hover>1</grid_config_hover>
	            </grid_element>
		</product_grid>		      
		<product_list>		     
	            <list_element>
	                  <list_gutter>1</list_gutter>
	                  <list_image_ratio>1</list_image_ratio>
	                  <list_custom_ratio_width>1</list_custom_ratio_width>
	                  <list_custom_ratio_height>1</list_custom_ratio_height>
	                  <list_alt_img>thumbnail</list_alt_img>
	                  <list_show_thumbnail>1</list_show_thumbnail>
	                  <list_show_product_name>1</list_show_product_name>
	                  <list_auto_product_name>1</list_auto_product_name>
	                  <list_show_desc>1</list_show_desc>
	                  <list_show_price>1</list_show_price>
	                  <list_show_review>1</list_show_review>
	                  <list_show_addto>1</list_show_addto>
	            </list_element>
		</product_list>
		<product_detail>
			<detail_element>
				<show_sku>1</show_sku>
				<use_tabs>1</use_tabs>
			</detail_element>
			<detail_image>
				<main_image_width></main_image_width>
				<main_image_height></main_image_height>
				<thumb_image_width></thumb_image_width>
				<thumb_image_height></thumb_image_height>
			</detail_image>
			<moreview_options>
				<loop>true</loop>
				<caption>true</caption>
				<allowfullscreen>true</allowfullscreen>
				<navdir>horizontal</navdir>
				<transition_effect>slide</transition_effect>
				<transition_duration></transition_duration>
			</moreview_options>
			<magnifier_options>
				<fullscreenzoom>5</fullscreenzoom>
				<top>0</top>
				<left>670</left>
				<width>510</width>
				<height>510</height>
				<eventtype>hover</eventtype>
				<status>true</status>
			</magnifier_options>				
		</product_detail>
	</default>
</config>
