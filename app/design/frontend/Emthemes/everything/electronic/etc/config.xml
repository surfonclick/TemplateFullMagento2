<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
	      <cms_home_page>everything-home-two-columns-right</cms_home_page>
		<general_section>
			<layout>
			      <small_logo_src>logo_small_computer.png</small_logo_src>
			      <enable_rtl>0</enable_rtl>
				<disable_responsive>1</disable_responsive>
				<box_wide>0</box_wide>
				<sticky_menu>1</sticky_menu>
				<default_top_navigation>0</default_top_navigation>
			</layout>
			<general_blocks>
				<handle_login_form>1</handle_login_form>
				<popup_block>2</popup_block>
			</general_blocks>
		</general_section>
		<css>
			<custom_css_group>
				<additional_css_file></additional_css_file>
				<custom_css>0</custom_css>
			</custom_css_group>				
		</css>
		<fonts>
			<fonts_group>
				<google_font_key>AIzaSyCZ6wqklkyJFXWvoxvA5Zv0SGzHTd6JbbI</google_font_key>
				<google_fonts>Poppins</google_fonts>
				<google_fonts_weights>200,300,400,500,600,700,800</google_fonts_weights>
				<general_font>400 12px/1.65 Poppins,Helvetica Neue, Helvetica, Arial, sans-serif</general_font>
				<h1_font>400 30px/1.15 Poppins, Helvetica Neue, Helvetica, Arial, sans-serif</h1_font>
				<h2_font>400 20px/1.35 Poppins, Helvetica Neue, Helvetica, Arial, sans-serif</h2_font>
				<h3_font>400 18px/1.35 Poppins, Helvetica Neue, Helvetica, Arial, sans-serif</h3_font>
				<h4_font>400 16px/1.35 Poppins, Helvetica Neue, Helvetica, Arial, sans-serif</h4_font>
				<h5_font>400 14px/1.35 Poppins, Helvetica Neue, Helvetica, Arial, sans-serif</h5_font>
				<h6_font>400 11px/1.35 Poppins,Helvetica, Arial, sans-serif</h6_font>
			</fonts_group>				
		</fonts>
		<page>
			<page_group>
				<page_bg_color>#ffffff</page_bg_color>
				<page_bg_image></page_bg_image>
				<page_bg_file></page_bg_file>
				<page_bg_position>0 0</page_bg_position>
				<page_bg_repeat>repeat</page_bg_repeat>
				<page_bg_attachment>fixed</page_bg_attachment>
				<box_shadow>0 0 5px #ffffff</box_shadow>
				<rounded_corner>5px</rounded_corner>
			</page_group>				
		</page>
		<header>
			<header_group>
				<header_bg_color>#ffffff</header_bg_color>
